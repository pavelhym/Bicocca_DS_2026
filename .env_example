# OpenAI API Configuration
# Required: Get your API key from https://platform.openai.com/api-keys
# Used for all AI agents (GPT-4o-mini, GPT-4o, GPT-5)
OPENAI_API_KEY=your_openai_api_key_here

# Exa Search API Configuration
# Required: Get your API key from https://exa.ai/
# Used for semantic web search and document retrieval
EXA_API_KEY=your_exa_api_key_here

# ScrapingAnt API Configuration
# Optional: Get your API key from https://www.scrapingant.com/
# Used as fallback for web scraping when direct URL access fails
# System will work without this, but may have reduced reliability for some web sources
SCRAPER_ANT_API_KEY=your_scrapingant_api_key_here

# ============================================================================
# Search Engine API Keys (for parsing algorithms and testing notebooks)
# ============================================================================
# These keys are used in search_results_receiving.ipynb for testing different
# search engines. All are optional unless you plan to use the parsing notebooks.

# Brave Search API
# Optional: Get your API key from https://brave.com/search/api/
# Used in search_results_receiving.ipynb for Brave search engine testing
BRAVE_API_KEY=your_brave_api_key_here

# Google Custom Search API
# Optional: Get your API key and CSE ID from https://developers.google.com/custom-search
# Used in search_results_receiving.ipynb for Google search testing
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CSE_ID=your_google_cse_id_here

# Serper API (Google Search alternative)
# Optional: Get your API key from https://serper.dev/
# Used in search_results_receiving.ipynb for Google Serper search testing
SERPER_API_KEY=your_serper_api_key_here

# SearchAPI
# Optional: Get your API key from https://www.searchapi.io/
# Used in search_results_receiving.ipynb for SearchAPI testing
SEARCHAPI_API_KEY=your_searchapi_api_key_here

# SerpAPI
# Optional: Get your API key from https://serpapi.com/
# Used in search_results_receiving.ipynb for SerpAPI testing
SERP_API=your_serpapi_api_key_here

# Tavily Search API
# Optional: Get your API key from https://tavily.com/
# Used in search_results_receiving.ipynb for Tavily search testing
TAVILY_API_KEY=your_tavily_api_key_here

# You.com API
# Optional: Get your API key from https://api.you.com/
# Used in search_results_receiving.ipynb for You.com search testing
YDC_API_KEY=your_youcom_api_key_here

# Jina AI API
# Optional: Get your API key from https://jina.ai/
# Used in search_results_receiving.ipynb for Jina AI search testing
JINA_API_KEY=your_jina_api_key_here

